<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>FluentXPath</PackageId>
    <Version>1.0.4</Version>
    <Authors>Jeferson Tenorio</Authors>
    <Company>Jeferson Tenorio</Company>
    <PackageIconUrl>https://cdn.rawgit.com/jefersonsv/FluentXPath/master/artifacts/logo.jpg</PackageIconUrl>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/jefersonsv/FluentXPath</PackageProjectUrl>
    <Description>Build query XPath with fluent style code</Description>
    <PackageTags>xpath fluent fluentxpath</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Properties\**" />
    <EmbeddedResource Remove="Properties\**" />
    <None Remove="Properties\**" />
  </ItemGroup>

</Project>
